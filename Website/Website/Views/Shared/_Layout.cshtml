<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="google-site-verification" content="ROLHn6sbO5rMyZfuN54DbclJxzNKKW8V8eHAIXZE7OU" />
    <title>@(string.IsNullOrWhiteSpace(ViewBag.MetaTitle) ? ViewBag.Title + " - Full House Entertainment" : ViewBag.MetaTitle)</title>

    <script>
    (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
            (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date(); a = s.createElement(o),
        m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

    ga('create', 'UA-18741404-1', 'auto');
    ga('send', 'pageview');
    </script>

    @if (IsSectionDefined("SEOMetaTags"))
    {
    @RenderSection("SEOMetaTags")
    }
    else
    {
    <meta name="description" content="Full House Entertainment is Upstate New York's premier entertainment specialists for weddings, photo booths, private parties, casinos, fundraising and more.">
    }

    <!-- Bootstrap -->
    @Styles.Render("~/Content/css")

    <link href="~/favicon.ico" rel="shortcut icon">

    @RenderSection("styles", required: false)

    <meta property="fb:app_id" content="392535901138304"/>
    <meta property="fb:pages" content="377024277194" />
    <meta name="author" content="Full House Entertainment">
    @if (IsSectionDefined("SocialMediaTags"))
    {
        @RenderSection("SocialMediaTags")
    }
    else
    {
    // use default
    <meta property="og:image" content="http://fullhouse-ent.com/Content/images/site/carousel/1-min.jpg" />
    <meta property="og:title" content="@ViewBag.Title - Full House Entertainment" />
    <meta property="og:url" content="@Request.Url.AbsoluteUri" />
    <meta property="og:site_name" content="Full House Entertainment" />
    <meta property="og:type" content="website" />
    <meta property="og:description" content="Known as Upstate New York’s premier entertainment specialists, Full House Entertainment promises you the most unique night of your life. We pride ourselves on ensuring that you and your guests have FUN while we take care of the rest."/>
    }
</head>

<body>
    @{
        Html.RenderPartial("_TopCarousel");
    }

    <div class="@(ViewBag.IsInnerPage as bool? ?? false ? "body inner" : "body")">
        <nav class="navbar navbar-default top" data-spy="affix">
            <div class="container">
                <div class="navbar-header">
                    <a class="navbar-brand" href="@Url.Action("Index", "Home")" title="Full House Entertainment"><img src="~/Content/images/site/fhe-logo.png" height="100%" alt="Full House Entertainment" /></a>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="nav navbar-nav navbar-right">
                    <a class="navbar-brand" target="_blank" title="Visit us on Facebook" href="https://www.facebook.com/FullHouseEntertainmentofRochesterNY/"><img src="~/Content/images/site/fb.png" height="100%" alt="Facebook" /></a>
                    <a class="navbar-brand" target="_blank" title="Visit us on Instagram" href="https://www.instagram.com/full_house_ent/"><img src="~/Content/images/site/instagram.png" height="100%" alt="Instagram" /></a>
                    <a class="navbar-brand" href="/#contact-us" title="Contact us">
                        <i class="fa fa-phone" aria-hidden="true" style="font-size: 36px; line-height: 30px; color: white;"></i>
                    </a>
                    <a class="navbar-brand" href="https://www.gigbuilder.com/cal/ny/fullhouseentertainment.nsf/login.html" title="Existing customer login" target="_blank">
                        <i class="fa fa-sign-in" aria-hidden="true" style="font-size: 36px; line-height: 30px; color: white;"></i>
                    </a>
                </div>
            </div><!-- /.container -->
        </nav>

        <div class="container">
            @RenderBody()
        </div>

        <nav class="navbar navbar-default bottom">
            <div class="container">
                <div class="navbar-header">
                    Full House Entertainment - Farmington, NY
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="nav navbar-nav navbar-right hidden-xs">
                    All Rights Reserved&nbsp;&nbsp;&copy; @DateTime.Now.Year
                </div>
            </div><!-- /.container -->
        </nav>
    </div>

    <div id="blueimp-gallery" class="blueimp-gallery">
        <div class="slides"></div>
        <h3 class="title"></h3>
        <a class="prev">‹</a>
        <a class="next">›</a>
        <a class="close">×</a>
        <a class="play-pause"></a>
        <ol class="indicator"></ol>
    </div>

    <!-- Login Modal -->
    <div class="modal fade" tabindex="-1" role="dialog" id="CustomerLoginModal">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <form action="https://www.gigbuilder.com/cal/ny/fullhouseentertainment.nsf/api_clientlogin?createdocument" method="post" name="clientlogin">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title">Customer Login</h4>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-sm-12">
                                <p><em>If you experience issues with login or forgot your password, please <a href="/#contact-us" title="Contact us" onclick="$('#CustomerLoginModal').modal('toggle')">contact us</a>.</em></p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <label for="exampleInputEmail1">Event Date</label>
                                        </div>
                                        <div class="col-sm-6 col-xs-4">
                                            <select name="Month" id="Month" class="form-control">
                                                <option value="0" selected="selected" disabled="disabled">--Month--</option>
                                                <option value="1">January</option>
                                                <option value="2">February</option>
                                                <option value="3">March</option>
                                                <option value="4">April</option>
                                                <option value="5">May</option>
                                                <option value="6">June</option>
                                                <option value="7">July</option>
                                                <option value="8">August</option>
                                                <option value="9">September</option>
                                                <option value="10">October</option>
                                                <option value="11">November</option>
                                                <option value="12">December</option>
                                            </select>
                                        </div>

                                        <div class="col-sm-3 col-xs-4">
                                            <select name="Day" id="Day" class="form-control">
                                                <option selected="selected" disabled="disabled">--Day--</option>
                                                @for (int i = 1; i <= 31; i++)
                                                {
                                                    <option>@i</option>
                                                }
                                            </select>
                                        </div>

                                        <div class="col-sm-3 col-xs-4">
                                            <select name="Year" id="Year" class="form-control">
                                                <option selected="selected" disabled="disabled">--Year--</option>
                                                @for (int i = 0; i < 6; i++)
                                                {
                                                    <option>@((DateTime.Today.Year - 1) + i)</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputPassword1">Password</label>
                                    <input name="Password" type="password" id="Password" placeholder="Password" class="form-control" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        <input type="submit" class="btn btn-primary" value="Login">
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- End Login Modal -->
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/customScripts")
    @RenderSection("scripts", required: false)
</body>
</html>
